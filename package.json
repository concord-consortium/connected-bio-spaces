{
  "name": "connected-bio-spaces",
  "version": "1.0.6",
  "description": "Concord Consortium Connected Bio Spaces",
  "main": "index.js",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/",
      "/.scss"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts",
      "/.scss"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --content-base dist/",
    "build": "npm-run-all lint:build clean build:webpack",
    "build:webpack": "webpack --mode production --devtool false",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:build": "tslint -c tslint-build.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint-build.json --fix 'src/**/*.{ts,tsx}'",
    "lint:unused": "tsc --noUnusedLocals --project .",
    "update-authoring-types": "node scripts/update-authoring-types.js",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watchAll",
    "test:coverage:watch": "jest --coverage --watchAll",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:full": "npm-run-all test test:cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/connected-bio-spaces.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/connected-bio-spaces/issues"
  },
  "homepage": "https://github.com/concord-consortium/connected-bio-spaces#readme",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^3.0.0",
    "@types/chart.js": "^2.7.40",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/flat": "0.0.28",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/query-string": "^6.1.0",
    "@types/rc-slider": "^8.6.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-jsonschema-form": "^1.0.11",
    "@types/uuid": "^3.4.4",
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "canvas-prebuilt": "^1.6.11",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cypress": "^3.4.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "json-schema-to-typescript": "^6.1.1",
    "mini-css-extract-plugin": "^0.4.2",
    "node-pre-gyp": "^0.11.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "style-loader": "^0.23.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "chart.js": "^2.7.3",
    "chartjs-plugin-annotation": "git+https://github.com/chartjs/chartjs-plugin-annotation.git#b22a69b6",
    "flat": "^4.1.0",
    "gifler": "^0.1.0",
    "iframe-phone": "^1.2.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.15",
    "markdown-to-jsx": "^6.9.0",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8",
    "mobx-state-tree": "^3.4.0",
    "organelle": "0.0.11",
    "patternomaly": "^1.3.2",
    "populations-react": "0.0.3",
    "populations.js": "^0.1.13",
    "query-string": "^6.1.0",
    "rc-slider": "^8.6.3",
    "react": "^16.5.0",
    "react-chartjs-2": "^2.7.4",
    "react-dimensions": "^1.3.1",
    "react-dom": "^16.5.0",
    "react-jsonschema-form": "^1.0.5",
    "screenfull": "^5.0.0",
    "svg-path-properties": "^0.4.7",
    "uuid": "^3.3.2",
    "wait-on": "^3.0.1"
  }
}
